<div>
    {{#each beer}}
    <div class="beer">
        <img src= {{this.image_url}} alt="">
        <h3>Nombre: {{this.name}}</h3>
        <h4>Tagline:{this.tagLIne}</h4>
        <p>Description:{{this.description}}</p>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p>Brewers Tips: {{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div>